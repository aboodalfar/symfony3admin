<table class="table table-bordered">
    <tbody>
    <th>Login no.</th>
    <th>Created At</th>
    <th>Actions</th>
    {%for account in object.getPortalUser.getTradingAccounts() %}
        <tr class="sonata-ba-view-container">
            <th>{{account.login}}</th>
            <td>{{account.createdAt|date('Y-m-d H:i ')}}</td>
            <td>
                <a href="{{admin.generateObjectUrl('trading_accounts', account) }}">
                    Edit
                </a>
            </td>
        </tr>
    {%endfor%}
    </tbody>
</table>
